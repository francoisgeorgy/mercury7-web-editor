{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "jquery": true
    },
    "extends": ["eslint:recommended"],
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2018
    },
    "rules": {
        "linebreak-style": ["error", "unix"],
        "no-prototype-builtins": "warn",
        "no-inline-comments": "off"
    }
}